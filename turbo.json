{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^db:generate",
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT",
        ".env*"
      ],
      "outputs": [
        ".svelte-kit/**",
        ".vercel/**"
      ]
    },
    "lint": {},
    "dev": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false,
      "persistent": true
    }
  }
}
