{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@ckm/contracts": [
        "../../packages/contracts/src"
      ],
      "@ckm/types": [
        "../packages/types/src"
      ]
    },
  },
  "exclude": [
    "node_modules"
  ]
}
